                     </children>
                     <opaqueInsets>
                        <Insets right="20.0" />
                     </opaqueInsets>
                  </HBox>
               </children>
            </VBox>
            <HBox alignment="CENTER" spacing="5.0">
               <children>
                  <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Player Name:">
                     <font>
                        <Font name="Impact" size="20.0" />
                     </font>
                  </Text>
                  <TextField fx:id="randPlayerName" />
               </children>
               <opaqueInsets>
                  <Insets right="20.0" />
               </opaqueInsets>
            </HBox>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="200.0">
         <children>
            <Button fx:id="startButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#startGame" text="Start Game" textAlignment="CENTER">
               <font>
                  <Font name="Impact" size="21.0" />
               </font>
            </Button>
            <Button fx:id="startRandomGame" mnemonicParsing="false" onAction="#startRandomGame" text="Random Game">
               <font>
                  <Font name="Impact" size="21.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
