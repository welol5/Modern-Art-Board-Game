<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="800.0" prefWidth="1300.0" spacing="30.0" style="-fx-background-color: #726952;" xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxmlgui.GUIDriver">
   <children>
      <Text fill="WHITE" stroke="#5b4c37" strokeLineJoin="ROUND" strokeType="OUTSIDE" strokeWidth="7.0" text="Modern Art" textAlignment="CENTER">
